{
  "name": "context-manager",
  "version": "0.0.0",
  "private": true,
  "description": "Workspace wrapper for the Context Manager CLI tooling.",
  "workspaces": [
    "scripts/context-manager"
  ],
  "scripts": {
    "build:cli": "npm --workspace scripts/context-manager run build",
    "clean:cli": "npm --workspace scripts/context-manager run clean",
    "lint:cli": "npm --workspace scripts/context-manager run lint",
    "cli": "npm run build:cli --silent && node scripts/context-manager/dist/cli.js"
  }
}
